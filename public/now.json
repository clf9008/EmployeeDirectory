{
    "version": 2,
    "alias": "clf9008-user-directory",
    "name": "user-directory",
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}